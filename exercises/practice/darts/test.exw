include darts.e 

set_test_verbosity(TEST_SHOW_ALL)

test_equal(score(9, -9),0,"Missed target")
test_equal(score(10, 0),1,"On the outer circle")
test_equal(score(0, -5),5,"On the middle circle")
test_equal(score(-1, 0),10,"On the inner circle")
test_equal(score(0,  0),10,"Exactly on centre")
test_equal(score(-.1,-.1),10,"Near the centre")
test_equal(score(.7,.7),10,"Just within the inner circle")
test_equal(score(-.8,.8),5,"Just outside the inner circle")
test_equal(score(3.5,-3.5),5,"Just within the middle circle")
test_equal(score(-3.6,-3.6),1,"Just outside the middle circle")
test_equal(score(7, -7),1,"Just within the outer circle")
test_equal(score(-7.1,7.1),0,"Just outside the outer circle")
test_equal(score(-4,.5),5,"Asymmetric position between the inner and middle circles")
test_summary() 
