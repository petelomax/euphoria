include simple-linked-list.e

set_test_verbosity(TEST_SHOW_ALL)
list_push(1)
test_equal(list,{1},"list contents correct after 1 push")
list_push(2)
test_equal(list,{1,2},"list contents correct after 2 pushes")
list_push(3)
test_equal(list,{1,2,3},"list contents correct after 3 pushes")
test_equal(list_pop(),3,"pop() 3")
test_equal(length(list),2,"length=2 after pop")
test_equal(list_pop(),2,"pop() 2")
test_equal(list_pop(),1,"pop() 1")
test_equal(length(list),0,"length=0 after 3 pops")
test_equal(list_pop(),"list empty","pop on empty returns error")
list_push(1)
list_push(2)
list_push(3)
{} = list_pop()
list_push(4)
list_push(5)
{} = list_pop()
list_push(6)
test_equal(list,{1,2,4,6},"list contents correct after mixed push/pop")
test_summary()

